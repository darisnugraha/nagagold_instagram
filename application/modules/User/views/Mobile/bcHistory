        <script>
              $(document).ready(function() {
                $("#keatas").hide();
                // memberikan efek fade in pada id #keatas
                $(function () {
                    $(window).scroll(function () {
                    if ($(this).scrollTop() > 100) {
                        $('#keatas').fadeIn();
                    } else {
                        $('#keatas').fadeOut();
                    }
                    });
                });
              })
        </script>